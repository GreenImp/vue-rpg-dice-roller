{
  "name": "vue-rpg-dice-roller",
  "version": "0.1.3",
  "description": "Plugin for VuePress that allows rolling dice",
  "main": "dist/vue-rpg-dice-roller.umd.js",
  "module": "src/DiceRoller.vue",
  "unpkg": "dist/vue-rpg-dice-roller.umd.min.js",
  "browser": {
    "./sfc": "src/DiceRoller.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib src/DiceRoller.vue",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GreenImp/vue-rpg-dice-roller.git"
  },
  "keywords": [
    "dice",
    "roll",
    "vue"
  ],
  "author": "GreenImp <info@greenimp.co.uk> (http://greenimp.co.uk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GreenImp/vue-rpg-dice-roller/issues"
  },
  "homepage": "https://github.com/GreenImp/vue-rpg-dice-roller#readme",
  "dependencies": {
    "rpg-dice-roller": "^4.1.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  }
}
